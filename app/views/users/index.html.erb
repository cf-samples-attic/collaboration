<div data-role="header">
  <%= link_to 'Home', home_index_path, :class => 'ui-btn-left', "data-icon" => "home" %>
  <h1>Listing users</h1>
</div>

<div data-role="content">
<ul data-role="listview" data-inset="true">
<% @users.each do |user| %>
  <li><%= link_to user.display_name, user_path(user) %></li>
<% end %>
</ul>
</div>




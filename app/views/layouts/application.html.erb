<!DOCTYPE html>
<html>
<head>
  <title>Collaboration</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<p id="notice"><%= notice %></p>
<p id="alert"><%= alert %></p>

<%- if current_user %>
    <div style="float:right"><%= current_user.display_name %> | <%= link_to "Logout", "/users/sign_out" %></div>
<%- else %>

<%- end %>
<%= yield %>

</body>
</html>
